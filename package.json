{
  "private": true,
  "name": "@undataforum/main-website",
  "description": "United Nations World Data Forum main website.",
  "version": "0.0.0",
  "author": "Thilo Maier <maier@un.org>",
  "homepage": "https://undataforum.org",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "build": "rm -rf ./public && gatsby build",
    "format": "pretty-quick",
    "inspect": "node --inspect-brk node_modules/.bin/gatsby develop",
    "lint": "eslint --ext .jsx,.js --ignore-path .gitignore .",
    "precommit": "npm run lint && pretty-quick --staged"
  },
  "dependencies": {
    "gatsby": "^2.0.0-beta.61",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "@mdotasia/eslint-config": "^0.5.0",
    "eslint": "^5.8.0",
    "husky": "^1.1.2",
    "prettier": "^1.11.1",
    "pretty-quick": "^1.4.1"
  }
}
