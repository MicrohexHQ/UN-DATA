    "gatsby-remark-smartypants": "^2.0.6",
    "gatsby-source-filesystem": "^2.0.1-beta.10",
    "gatsby-transformer-remark": "^2.1.11",
    "gatsby-transformer-sharp": "^2.1.7",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "rehype-react": "^3.0.3"
  },
  "devDependencies": {
    "@mdotasia/eslint-config": "^0.5.0",
