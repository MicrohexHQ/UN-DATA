{
  "private": true,
  "name": "@undataforum/main-website",
  "description": "United Nations World Data Forum main website.",
  "version": "0.0.0",
  "author": "Thilo Maier <maier@un.org>",
  "homepage": "https://undataforum.org",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "now-build": "gatsby build",
    "clean": "rm -rf ./node_modules && rm -rf ./.cache && rm -rf ./public",
    "lint": "eslint . --ignore-path .gitignore",
    "format": "pretty-quick",
    "inspect": "node --inspect-brk node_modules/.bin/gatsby develop"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "dependencies": {
    "gatsby": "^2.0.0-beta.61",
    "gatsby-theme-undataforum": "^0.1.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "eslint": "^5.8.0",
    "eslint-plugin-shopify": "^28.0.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.11.1",
    "pretty-quick": "^1.4.1"
  }
}
