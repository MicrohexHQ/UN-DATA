{
  "private": true,
  "name": "@undataforum/website",
  "description": "Main UN Data Forum website.",
  "version": "0.0.0",
  "author": {
    "name": "Thilo Maier",
    "email": "maier@un.org"
  },
  "homepage": "https://www.undataforum.org",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
